{
    "app": {
        "app_name": "HiDanny",
        "pipeline": {
            "app_type": "DOCKER_RUN",
            "build_jobs": {
                "build_target": "http://build.com",
                "version_control": {
                    "branch": "master",
                    "type": "github",
                    "url": "http://somerepo.github.com"
                }
            },
            "deploy_jobs": {
                "deploy_commands": "docker-compose up -d",
                "deploy_target": "http://deliver.com"
            },
            "test_jobs": {
			"unit_test": {
				"test_command": "npm test"
                	}
            }
        }
    },
    "jenkins_version": "2.89.2"
}